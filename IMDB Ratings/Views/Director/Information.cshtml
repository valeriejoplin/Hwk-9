@model IMDB_Ratings.Models.Director

@{
    ViewBag.Title = "Information";
}

<h2>
    Director Information
</h2>
<h3> @Model.Director_Name</h3>
<p>
    Movies directed include:
    <ul>
        @foreach (var movie in Model.Movies)
        {
           
                <li>
                    <a href="@movie.movie_imdb_link"> @movie.movie_title </a>(@movie.title_year)
                    
                </li>
                }
            
        </ul>
</p>
